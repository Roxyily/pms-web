{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nimport { mapState } from 'vuex';\nexport default {\n  name: \"HouseholdMain\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 8,\n      total: 0,\n      feeItemName: null,\n      centerDialogVisible: false,\n      form: {\n        id: '',\n        name: '',\n        contact: '',\n        unitNumber: '',\n        roomNumber: '',\n        moveInDate: '',\n        moveOutDate: ''\n      },\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入姓名',\n          trigger: 'blur'\n        }],\n        contact: [{\n          required: true,\n          message: '联系方式不能空',\n          trigger: 'blur'\n        }, {\n          pattern: /^1[3|4|5|6|7|8|9][0-9]\\d{8}$/,\n          message: '请输入有效手机号',\n          trigger: 'blur'\n        }],\n        unitNumber: [{\n          required: true,\n          message: '请输入单元号',\n          trigger: 'blur'\n        }, {\n          min: 1,\n          max: 2,\n          message: '长度在 1 到 2 个字符',\n          trigger: 'blur'\n        }],\n        roomNumber: [{\n          required: true,\n          message: '请输入房间号',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 4,\n          message: '长度在 3 到 4 个字符',\n          trigger: 'blur'\n        }]\n      },\n      delVisible: false,\n      delID: [],\n      // 存放删除的数据的id\n      multipleSelection: [],\n      // 多选的数据\n      editVisible: false,\n      editForm: {\n        amount: '',\n        feeItemName: '',\n        feeItemId: '',\n        propertyId: ''\n      },\n      editConfirm: false\n    };\n  },\n  computed: {\n    ...mapState(['residentList'])\n  },\n  methods: {\n    //每一页可显示的记录数更改时run\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.pageNum = 1;\n      this.pageSize = val;\n      this.loadPost();\n    },\n    //分页时页数切换\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    },\n    //表格记录的序号\n    getIndex(index) {\n      return (this.pageNum - 1) * this.pageSize + index + 1;\n    },\n    //重新加载表单\n    resetFrom() {\n      this.$refs.form.resetFields();\n    },\n    //多选操作\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    //批量删除\n    del() {\n      this.delVisible = true; // 显示删除弹框\n      for (let i = 0; i < this.multipleSelection.length; i++) {\n        this.delID.push(this.multipleSelection[i].feeItemId);\n      }\n    },\n    //确认删除\n    delAll() {\n      for (let i = 0; i < this.delID.length; i++) {\n        this.$axios.get(this.$httpUrl + '/fee-item/delete?id=' + this.delID[i]).then(res => {\n          console.log(res);\n          console.log(res.status);\n          if (res.status === 200) {\n            console.log('删除成功');\n            this.delVisible = false;\n            this.$message({\n              message: '删除成功！',\n              type: 'success'\n            });\n            this.loadPost();\n          } else {\n            this.$message.error('删除失败！');\n          }\n        });\n      }\n    },\n    //添加记录\n    doSave() {\n      this.$axios.post(this.$httpUrl + '/resident/save', this.form).then(res => {\n        console.log(res);\n        if (res.data.code === 200) {\n          this.$message({\n            message: '操作成功！',\n            type: 'success'\n          });\n          this.centerDialogVisible = false;\n          this.loadPost();\n          this.resetFrom();\n        } else if (res.data.code === 400) {\n          if (res.data.data === \"User is not logged in\") {\n            this.$message({\n              message: '操作失败！',\n              type: 'error'\n            });\n          } else if (res.data.data === \"Data already exists\") {\n            this.$message({\n              message: '房间已存在！',\n              type: 'error'\n            });\n          }\n        }\n      });\n    },\n    //修改记录\n    doMod() {\n      console.log('@@@@@@@');\n      console.log(this.editForm.amount);\n      this.$axios.post(this.$httpUrl + '/fee-item/update', {\n        amount: this.editForm.amount,\n        id: this.editForm.feeItemId,\n        name: this.editForm.feeItemName,\n        propertyId: this.editForm.propertyId\n      }).then(res => {\n        console.log(res);\n        if (res.data.code === 200) {\n          this.$message({\n            message: '操作成功！',\n            type: 'success'\n          });\n          this.editVisible = false;\n          this.editConfirm = false;\n          this.loadPost();\n          this.resetFrom();\n        } else if (res.data.code === 400) {\n          if (res.data.data === \"User is not logged in\") {\n            this.$message({\n              message: '操作失败！',\n              type: 'error'\n            });\n          }\n        }\n      });\n    },\n    //根据是添加记录还是修改记录提交不同的表单\n    save(type) {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          if (type === 'edit') {\n            this.doMod();\n          } else if (type === 'update') {\n            this.doSave();\n          }\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n    add() {\n      this.centerDialogVisible = true;\n      this.$nextTick(() => {\n        this.resetFrom();\n      });\n    },\n    resetParam() {\n      this.amount = null;\n      this.feeItemName = null;\n    },\n    editRow(row) {\n      this.editForm = Object.assign({}, row); // 将当前行的数据复制给表单的数据\n      this.editVisible = true; // 显示修改表单\n    },\n\n    //加载列表\n    loadPost() {\n      this.$axios.post(this.$httpUrl + '/fee-item/listPage', {\n        pageSize: this.pageSize,\n        pageNum: this.pageNum,\n        param: {\n          feeItemName: this.feeItemName\n        }\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.code === 200) {\n          this.tableData = res.data.data;\n          this.total = res.data.total;\n        } else {\n          alert('获取数据失败');\n        }\n      });\n    }\n  },\n  beforeMount() {\n    this.loadPost();\n  }\n};","map":{"version":3,"names":["mapState","name","data","tableData","pageNum","pageSize","total","feeItemName","centerDialogVisible","form","id","contact","unitNumber","roomNumber","moveInDate","moveOutDate","rules","required","message","trigger","pattern","min","max","delVisible","delID","multipleSelection","editVisible","editForm","amount","feeItemId","propertyId","editConfirm","computed","methods","handleSizeChange","val","console","log","loadPost","handleCurrentChange","getIndex","index","resetFrom","$refs","resetFields","handleSelectionChange","del","i","length","push","delAll","$axios","get","$httpUrl","then","res","status","$message","type","error","doSave","post","code","doMod","save","validate","valid","add","$nextTick","resetParam","editRow","row","Object","assign","param","alert","beforeMount"],"sources":["src/components/FeeItem/FeeItemMain.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <div class=\"searchBox\">\r\n        <el-input v-model=\"feeItemName\" placeholder=\"请输入收费项目\" style=\"width: 200px\"\r\n                  @keyup.enter.native=\"loadPost\"></el-input>\r\n\r\n        <span class=\"box1\">\r\n           <el-button type=\"warning\" plain  @click=\"loadPost\">\r\n            <i class=\"el-icon-search\"></i>\r\n          </el-button>\r\n          <el-button type=\"warning\" plain @click=\"resetParam\">重置</el-button>\r\n        </span>\r\n\r\n        <div class=\"box2\">\r\n          <el-button type=\"warning\" plain @click=\"del\">删除</el-button>\r\n          <!--          <el-button  type=\"warning\" plain style=\"margin-left: 5px\" @click=\"add\">新增</el-button>-->\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!--    数据表格-->\r\n    <el-table :data=\"tableData\"\r\n              @selection-change=\"handleSelectionChange\"\r\n              :select-on-indeterminate=true\r\n              :stripe=true\r\n              :border=true\r\n              :index-method=\"getIndex\"\r\n    >\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column type=\"index\" index=“getIndex”>\r\n      </el-table-column>\r\n      <el-table-column prop=\"communityId\" label=\"小区号\" >\r\n      </el-table-column>\r\n      <el-table-column prop=\"feeItemName\" label=\"收费项目\" >\r\n      </el-table-column>\r\n      <el-table-column prop=\"amount\" label=\"收费金额（￥）\" >\r\n      </el-table-column>\r\n      <el-table-column prop=\"operation\" label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <i class=\"el-icon-edit\" @click=\"editRow(scope.row)\"></i>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--    分页管理器-->\r\n    <div class=\"block\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=pageNum\r\n          :page-sizes=\"[5, 10, 20, 30]\"\r\n          :page-size=pageSize\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=this.total>\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <!--    添加记录的表单-->\r\n    <div>\r\n      <el-dialog\r\n          title=\"新增\"\r\n          :visible.sync=\"centerDialogVisible\"\r\n          width=\"30%\"\r\n          center\r\n      >\r\n        <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"80px\">\r\n          <el-form-item label=\"姓名\" prop=\"name\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.name\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"联系方式\" prop=\"contact\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.contact\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"单元号\" prop=\"unitNumber\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.unitNumber\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"房间号\" prop=\"roomNumber\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.roomNumber\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-row>\r\n            <el-col :span=\"12\">\r\n              <el-form-item label=\"入住时间\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.moveInDate\" style=\"width: 100%;\"></el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"12\">\r\n              <el-form-item label=\"离开时间\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.moveOutDate\" style=\"width: 100%;\"></el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save('update')\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n\r\n    <!-- 删除提示框 -->\r\n    <el-dialog title=\"提示\"\r\n               :visible.sync=\"delVisible\"\r\n               width=\"300px\"\r\n               center\r\n    >\r\n      <div class=\"del-dialog-cnt\">删除不可恢复，是否确定删除？</div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"delVisible = false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"delAll\">确 定</el-button>\r\n    </span>\r\n    </el-dialog>\r\n\r\n    <!--编辑一条信息的表单-->\r\n    <div>\r\n      <el-dialog\r\n          title=\"编辑信息\"\r\n          :visible.sync=\"editVisible\"\r\n          width=\"30%\"\r\n          center\r\n      >\r\n        <el-form ref=\"form\" :rules=\"rules\" :model=\"editForm\" label-width=\"80px\">\r\n          <el-form-item label=\"收费项目\" prop=\"feeItemName\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"editForm.feeItemName\">{{}}</el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"收费金额\" prop=\"amount\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"editForm.amount\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"editVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editConfirm =true\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n\r\n    <!-- 确定更改信息提示框 -->\r\n    <el-dialog title=\"提示\"\r\n               :visible.sync=\"editConfirm\"\r\n               width=\"300px\"\r\n               center\r\n    >\r\n      <div class=\"del-dialog-cnt\">是否更改这条信息?</div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"editConfirm = false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"save('edit')\">确 定</el-button>\r\n    </span>\r\n    </el-dialog>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex'\r\nexport default {\r\n  name: \"HouseholdMain\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      pageNum:1,\r\n      pageSize:8,\r\n      total:0,\r\n      feeItemName:null,\r\n      centerDialogVisible: false,\r\n      form:{\r\n        id:'',\r\n        name:'',\r\n        contact:'',\r\n        unitNumber:'',\r\n        roomNumber:'',\r\n        moveInDate:'',\r\n        moveOutDate:''\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入姓名', trigger: 'blur' }\r\n        ],\r\n        contact: [\r\n          { required: true, message: '联系方式不能空', trigger: 'blur' },\r\n          { pattern: /^1[3|4|5|6|7|8|9][0-9]\\d{8}$/, message: '请输入有效手机号', trigger: 'blur' }\r\n        ],\r\n        unitNumber: [\r\n          { required: true, message: '请输入单元号', trigger: 'blur' },\r\n          { min: 1, max: 2, message: '长度在 1 到 2 个字符', trigger: 'blur' }\r\n        ],\r\n        roomNumber: [\r\n          { required: true, message: '请输入房间号', trigger: 'blur' },\r\n          { min: 3, max: 4, message: '长度在 3 到 4 个字符', trigger: 'blur' }\r\n        ]\r\n      },\r\n      delVisible:false,\r\n      delID: [], // 存放删除的数据的id\r\n      multipleSelection: [], // 多选的数据\r\n      editVisible:false,\r\n      editForm:{\r\n        amount:'',\r\n        feeItemName:'',\r\n        feeItemId:'',\r\n        propertyId:'',\r\n      },\r\n      editConfirm:false,\r\n    }\r\n  },\r\n  computed:{\r\n    ...mapState(['residentList'])\r\n  },\r\n  methods:{\r\n    //每一页可显示的记录数更改时run\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.pageNum=1;\r\n      this.pageSize=val;\r\n      this.loadPost();\r\n    },\r\n\r\n    //分页时页数切换\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.pageNum=val;\r\n      this.loadPost();\r\n    },\r\n\r\n    //表格记录的序号\r\n    getIndex(index){\r\n      return (this.pageNum - 1) * this.pageSize + index + 1;\r\n    },\r\n\r\n    //重新加载表单\r\n    resetFrom(){\r\n      this.$refs.form.resetFields();\r\n    },\r\n\r\n    //多选操作\r\n    handleSelectionChange(val){\r\n      this.multipleSelection = val;\r\n    },\r\n\r\n    //批量删除\r\n    del(){\r\n      this.delVisible = true; // 显示删除弹框\r\n      for(let i=0;i<this.multipleSelection.length;i++)\r\n      {\r\n        this.delID.push(this.multipleSelection[i].feeItemId)\r\n      }\r\n    },\r\n\r\n    //确认删除\r\n    delAll(){\r\n      for(let i=0;i<this.delID.length;i++)\r\n      {\r\n        this.$axios.get(this.$httpUrl + '/fee-item/delete?id='+this.delID[i], ).then(res=>{\r\n          console.log(res);\r\n          console.log(res.status);\r\n          if(res.status===200){\r\n            console.log('删除成功');\r\n            this.delVisible=false;\r\n            this.$message({\r\n              message: '删除成功！',\r\n              type: 'success'\r\n            });\r\n            this.loadPost()\r\n          }else{\r\n            this.$message.error('删除失败！');\r\n          }\r\n        },)\r\n      }\r\n    },\r\n\r\n    //添加记录\r\n    doSave(){\r\n      this.$axios.post(this.$httpUrl + '/resident/save',this.form).then(res=>{\r\n        console.log(res);\r\n        if(res.data.code===200){\r\n          this.$message({\r\n            message: '操作成功！',\r\n            type: 'success'\r\n          });\r\n          this.centerDialogVisible = false\r\n          this.loadPost()\r\n          this.resetFrom()\r\n        }else if(res.data.code===400){\r\n          if(res.data.data===\"User is not logged in\"){\r\n            this.$message({\r\n              message: '操作失败！',\r\n              type: 'error'\r\n            });\r\n          } else if(res.data.data===\"Data already exists\"){\r\n            this.$message({\r\n              message: '房间已存在！',\r\n              type: 'error'\r\n            });\r\n          }\r\n        }\r\n      })\r\n    },\r\n\r\n    //修改记录\r\n    doMod(){\r\n      console.log('@@@@@@@')\r\n      console.log(this.editForm.amount)\r\n      this.$axios.post(this.$httpUrl + '/fee-item/update',\r\n          {\r\n            amount:this.editForm.amount,\r\n            id:this.editForm.feeItemId,\r\n            name:this.editForm.feeItemName,\r\n            propertyId:this.editForm.propertyId,\r\n          }).then(res=>{\r\n        console.log(res);\r\n        if(res.data.code===200){\r\n          this.$message({\r\n            message: '操作成功！',\r\n            type: 'success'\r\n          });\r\n          this.editVisible = false\r\n          this.editConfirm = false\r\n          this.loadPost()\r\n          this.resetFrom()\r\n        }else if(res.data.code===400){\r\n          if(res.data.data===\"User is not logged in\"){\r\n            this.$message({\r\n              message: '操作失败！',\r\n              type: 'error'\r\n            });\r\n          }\r\n        }\r\n      })\r\n    },\r\n\r\n    //根据是添加记录还是修改记录提交不同的表单\r\n    save(type){\r\n      this.$refs.form.validate((valid) => {\r\n        if (valid) {\r\n          if(type==='edit'){\r\n            this.doMod()\r\n          }else if (type==='update'){\r\n            this.doSave()\r\n          }\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      })\r\n    },\r\n\r\n\r\n    add(){\r\n      this.centerDialogVisible = true;\r\n      this.$nextTick(()=>{\r\n        this.resetFrom()\r\n      })\r\n    },\r\n\r\n    resetParam(){\r\n      this.amount=null;\r\n      this.feeItemName=null;\r\n    },\r\n\r\n    editRow(row){\r\n      this.editForm = Object.assign({}, row); // 将当前行的数据复制给表单的数据\r\n      this.editVisible = true; // 显示修改表单\r\n    },\r\n\r\n    //加载列表\r\n    loadPost(){\r\n      this.$axios.post(this.$httpUrl + '/fee-item/listPage', {\r\n        pageSize:this.pageSize,\r\n        pageNum:this.pageNum,\r\n        param:{\r\n          feeItemName:this.feeItemName,\r\n        }\r\n      }).then(res=>{\r\n        console.log(res.data);\r\n        if(res.data.code===200){\r\n          this.tableData=res.data.data;\r\n          this.total=res.data.total;\r\n        }else{\r\n          a