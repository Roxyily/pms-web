{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nimport { mapState } from 'vuex';\nexport default {\n  name: \"HouseholdMain\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      // showBox:false,\n      name: '',\n      unitNumber: '',\n      roomNumber: '',\n      // icon: 'el-icon-caret-bottom',\n      // content:'下拉查询',\n      // placement:'top',\n      centerDialogVisible: false,\n      form: {\n        id: '',\n        name: '',\n        contact: '',\n        unitNumber: '',\n        roomNumber: '',\n        moveInDate: '',\n        moveOutDate: ''\n      },\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入姓名',\n          trigger: 'blur'\n        }],\n        contact: [{\n          required: true,\n          message: '联系方式不能空',\n          trigger: 'blur'\n        }, {\n          pattern: /^1[3|4|5|6|7|8|9][0-9]\\d{8}$/,\n          message: '请输入有效手机号',\n          trigger: 'blur'\n        }],\n        unitNumber: [{\n          required: true,\n          message: '请输入单元号',\n          trigger: 'blur'\n        }, {\n          min: 1,\n          max: 2,\n          message: '长度在 1 到 2 个字符',\n          trigger: 'blur'\n        }],\n        roomNumber: [{\n          required: true,\n          message: '请输入房间号',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 4,\n          message: '长度在 3 到 4 个字符',\n          trigger: 'blur'\n        }]\n      },\n      delVisible: false,\n      delID: [],\n      // 存放删除的数据的id\n      multipleSelection: [],\n      // 多选的数据\n      editVisible: false,\n      editForm: {\n        id: '',\n        name: '',\n        contact: '',\n        unitNumber: '',\n        roomNumber: '',\n        moveInDate: '',\n        moveOutDate: ''\n      }\n    };\n  },\n  computed: {\n    ...mapState(['residentList'])\n  },\n  methods: {\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.pageNum = 1;\n      this.pageSize = val;\n      this.loadPost();\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    },\n    handleClose(done) {\n      this.$confirm('确认关闭？').then(() => {\n        done();\n      }).catch(() => {});\n    },\n    resetFrom() {\n      this.$refs.form.resetFields();\n    },\n    //多选操作\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    //批量删除\n    del() {\n      this.delVisible = true; // 显示删除弹框\n      for (let i = 0; i < this.multipleSelection.length; i++) {\n        this.delID.push(this.multipleSelection[i].id);\n      }\n    },\n    //确认删除\n    delAll() {\n      for (let i = 0; i < this.delID.length; i++) {\n        this.$axios.get(this.$httpUrl + '/resident/delete', {\n          id: this.delID[i]\n        }).then(res => {\n          console.log(res);\n          console.log(res.data.code);\n          if (res.status === 200) {\n            console.log('删除成功');\n          } else {\n            alert('删除失败');\n          }\n        });\n      }\n    },\n    mod(row) {\n      console.log(row);\n      this.form.id = row.id;\n      this.form.name = row.name;\n      this.form.contact = row.contact;\n      this.form.unitNumber = row.unitNumber;\n      this.form.roomNumber = row.roomNumber;\n      this.form.moveInDate = row.moveInDate;\n      this.form.moveOutDate = row.moveOutDate;\n      this.centerDialogVisible = true;\n    },\n    doSave() {\n      this.$axios.post(this.$httpUrl + '/resident/save', this.form).then(res => {\n        console.log(res);\n        if (res.data.code == 200) {\n          this.$message({\n            message: '操作成功！',\n            type: 'success'\n          });\n          this.centerDialogVisible = false;\n          this.loadPost();\n          this.resetFrom();\n        } else if (res.data.code == 400) {\n          if (res.data.data == \"User is not logged in\") {\n            this.$message({\n              message: '操作失败！',\n              type: 'error'\n            });\n          } else if (res.data.data == \"Data already exists\") {\n            this.$message({\n              message: '房间已存在！',\n              type: 'error'\n            });\n          }\n        }\n      });\n    },\n    doMod() {\n      this.$axios.post(this.$httpUrl + '/resident/update', this.form).then(res => {\n        console.log(res);\n        if (res.data.code == 200) {\n          this.$message({\n            message: '操作成功！',\n            type: 'success'\n          });\n          this.centerDialogVisible = false;\n          this.loadPost();\n          this.resetFrom();\n        } else if (res.data.code == 400) {\n          if (res.data.data == \"User is not logged in\") {\n            this.$message({\n              message: '操作失败！',\n              type: 'error'\n            });\n          }\n        }\n      });\n    },\n    save() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          if (this.form.id) {\n            this.doMod();\n          } else {\n            this.doSave();\n          }\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n    add() {\n      this.centerDialogVisible = true;\n      this.$nextTick(() => {\n        this.resetFrom();\n      });\n    },\n    resetParam() {\n      this.name = '';\n      this.unitNumber = '';\n      this.roomNumber = '';\n    },\n    editClose(done) {\n      this.$confirm('确认关闭？').then(() => {\n        done();\n      }).catch(() => {});\n    },\n    //加载列表\n    loadPost() {\n      this.$axios.post(this.$httpUrl + '/resident/listPage', {\n        pageSize: this.pageSize,\n        pageNum: this.pageNum,\n        param: {\n          name: this.name,\n          unitNumber: this.unitNumber,\n          roomNumber: this.roomNumber\n        }\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.code == 200) {\n          this.tableData = res.data.data;\n          this.total = res.data.total;\n        } else {\n          alert('获取数据失败');\n        }\n      });\n    }\n    // doFan(){\n    //   this.showBox = !this.showBox\n    //   if(!this.showBox){\n    //     this.icon='el-icon-caret-bottom'\n    //     this.placement='bottom'\n    //     this.content='下拉查询'\n    //   }else{\n    //     this.icon='el-icon-caret-top'\n    //     this.placement='top'\n    //     this.content='上拉隐藏'\n    //   }\n    // },\n  },\n\n  beforeMount() {\n    this.loadPost();\n  }\n};","map":{"version":3,"names":["mapState","name","data","tableData","pageNum","pageSize","total","unitNumber","roomNumber","centerDialogVisible","form","id","contact","moveInDate","moveOutDate","rules","required","message","trigger","pattern","min","max","delVisible","delID","multipleSelection","editVisible","editForm","computed","methods","handleSizeChange","val","console","log","loadPost","handleCurrentChange","handleClose","done","$confirm","then","catch","resetFrom","$refs","resetFields","handleSelectionChange","del","i","length","push","delAll","$axios","get","$httpUrl","res","code","status","alert","mod","row","doSave","post","$message","type","doMod","save","validate","valid","add","$nextTick","resetParam","editClose","param","beforeMount"],"sources":["src/components/HouseholdManage/HouseholdMain.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <div class=\"searchBox\">\r\n        <el-input v-model=\"name\" placeholder=\"请输入名字\" style=\"width: 200px\"\r\n                  @keyup.enter.native=\"loadPost\"></el-input>\r\n        <el-input v-model=\"unitNumber\" placeholder=\"输入单元\" style=\"width: 100px\"\r\n                  @keyup.enter.native=\"loadPost\"></el-input>\r\n        <el-input v-model=\"roomNumber\" placeholder=\"输入房间\" style=\"width: 100px\"\r\n                  @keyup.enter.native=\"loadPost\"></el-input>\r\n        <span class=\"box1\">\r\n           <el-button type=\"warning\" plain  @click=\"loadPost\">\r\n            <i class=\"el-icon-search\"></i>\r\n          </el-button>\r\n          <el-button type=\"warning\" plain @click=\"resetParam\">重置</el-button>\r\n        </span>\r\n\r\n        <div class=\"box2\">\r\n          <el-button type=\"warning\" plain @click=\"del\">删除</el-button>\r\n          <el-button  type=\"warning\" plain style=\"margin-left: 5px\" @click=\"add\">新增</el-button>\r\n        </div>\r\n      </div>\r\n\r\n<!--      <div>-->\r\n<!--        <el-tooltip class=\"item\" effect=\"dark\" :content=content :placement=placement>-->\r\n<!--          <i :class=icon @click=\"doFan\" style=\"margin-left: 10px; size: 100px; font-size: 30px\"></i>-->\r\n<!--        </el-tooltip>-->\r\n<!--      </div>-->\r\n\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\"\r\n              @selection-change=\"handleSelectionChange\"\r\n              :select-on-indeterminate=true\r\n              :stripe=true\r\n              :border=true\r\n    >\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"序号\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"communityId\" label=\"小区号\" width=\"100\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"moveInDate\" label=\"入住时间\" width=\"150\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"moveOutDate\" label=\"搬离时间\" width=\"150\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"contact\" label=\"手机号\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"roomNumber\" label=\"房间号\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"unitNumber\" label=\"单元号\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"operation\" label=\"操作\" width=\"80\">\r\n        <i class=\"el-icon-edit\" @click=\"editVisible = true\"></i>\r\n      </el-table-column>\r\n    </el-table>\r\n    \r\n    <div class=\"block\">\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=pageNum\r\n        :page-sizes=\"[5, 10, 20, 30]\"\r\n        :page-size=pageSize\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=this.total>\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <!--    添加记录的表单-->\r\n    <div>\r\n      <el-dialog\r\n          title=\"新增\"\r\n          :visible.sync=\"centerDialogVisible\"\r\n          width=\"30%\"\r\n          center\r\n          :before-close=\"handleClose\">\r\n        <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"80px\">\r\n          <el-form-item label=\"姓名\" prop=\"name\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.name\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"联系方式\" prop=\"contact\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.contact\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"单元号\" prop=\"unitNumber\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.unitNumber\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"房间号\" prop=\"roomNumber\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.roomNumber\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-row>\r\n            <el-col :span=\"12\">\r\n              <el-form-item label=\"入住时间\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.moveInDate\" style=\"width: 100%;\"></el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"12\">\r\n              <el-form-item label=\"离开时间\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.moveOutDate\" style=\"width: 100%;\"></el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n\r\n    <!-- 删除提示框 -->\r\n    <el-dialog title=\"提示\"\r\n               :visible.sync=\"delVisible\"\r\n               width=\"300px\"\r\n               center\r\n    >\r\n      <div class=\"del-dialog-cnt\">删除不可恢复，是否确定删除？</div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"delVisible = false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"delAll\">确 定</el-button>\r\n    </span>\r\n    </el-dialog>\r\n\r\n    <!--编辑一条信息的表单-->\r\n    <div>\r\n      <el-dialog\r\n          title=\"编辑信息\"\r\n          :visible.sync=\"editVisible\"\r\n          width=\"30%\"\r\n          center\r\n          :before-close=\"editClose\"\r\n          v-show=\"false\"\r\n      >\r\n        <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"80px\">\r\n          <el-form-item label=\"姓名\" prop=\"name\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.name\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"联系方式\" prop=\"contact\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.contact\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"单元号\" prop=\"unitNumber\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.unitNumber\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"房间号\" prop=\"roomNumber\">\r\n            <el-col :span=\"20\">\r\n              <el-input v-model=\"form.roomNumber\"></el-input>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-row>\r\n            <el-col :span=\"12\">\r\n              <el-form-item label=\"入住时间\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.moveInDate\" style=\"width: 100%;\"></el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"12\">\r\n              <el-form-item label=\"离开时间\">\r\n                <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.moveOutDate\" style=\"width: 100%;\"></el-date-picker>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"editVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n \r\n  \r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex'\r\nexport default {\r\n  name: \"HouseholdMain\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      pageNum:1,\r\n      pageSize:10,\r\n      total:0,\r\n      // showBox:false,\r\n      name:'',\r\n      unitNumber:'',\r\n      roomNumber:'',\r\n      // icon: 'el-icon-caret-bottom',\r\n      // content:'下拉查询',\r\n      // placement:'top',\r\n      centerDialogVisible: false,\r\n      form:{\r\n        id:'',\r\n        name:'',\r\n        contact:'',\r\n        unitNumber:'',\r\n        roomNumber:'',\r\n        moveInDate:'',\r\n        moveOutDate:''\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入姓名', trigger: 'blur' }\r\n        ],\r\n        contact: [\r\n          { required: true, message: '联系方式不能空', trigger: 'blur' },\r\n          { pattern: /^1[3|4|5|6|7|8|9][0-9]\\d{8}$/, message: '请输入有效手机号', trigger: 'blur' }\r\n        ],\r\n        unitNumber: [\r\n          { required: true, message: '请输入单元号', trigger: 'blur' },\r\n          { min: 1, max: 2, message: '长度在 1 到 2 个字符', trigger: 'blur' }\r\n        ],\r\n        roomNumber: [\r\n          { required: true, message: '请输入房间号', trigger: 'blur' },\r\n          { min: 3, max: 4, message: '长度在 3 到 4 个字符', trigger: 'blur' }\r\n        ]\r\n      },\r\n      delVisible:false,\r\n      delID: [], // 存放删除的数据的id\r\n      multipleSelection: [], // 多选的数据\r\n      editVisible:false,\r\n      editForm:{\r\n        id:'',\r\n        name:'',\r\n        contact:'',\r\n        unitNumber:'',\r\n        roomNumber:'',\r\n        moveInDate:'',\r\n        moveOutDate:''\r\n      },\r\n    }\r\n  },\r\n  computed:{\r\n    ...mapState(['residentList'])\r\n  },\r\n  methods:{\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.pageNum=1;\r\n      this.pageSize=val;\r\n      this.loadPost();\r\n    },\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.pageNum=val;\r\n      this.loadPost();\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm('确认关闭？')\r\n          .then(() => {\r\n            done();\r\n          })\r\n          .catch(() => {});\r\n    },\r\n    resetFrom(){\r\n      this.$refs.form.resetFields();\r\n    },\r\n    //多选操作\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection = val;\r\n    },\r\n    //批量删除\r\n    del(){\r\n      this.delVisible = true; // 显示删除弹框\r\n      for(let i=0;i<this.multipleSelection.length;i++)\r\n      {\r\n        this.delID.push(this.multipleSelection[i].id)\r\n      }\r\n    },\r\n    //确认删除\r\n    delAll(){\r\n      for(let i=0;i<this.delID.length;i++)\r\n      {\r\n        this.$axios.get(this.$httpUrl + '/resident/delete', {\r\n          id:this.delID[i]\r\n        }).then(res=>{\r\n          console.log(res);\r\n          console.log(res.data.code);\r\n          if(res.status===200){\r\n            console.log('删除成功');\r\n          }else{\r\n            alert('删除失败');\r\n          }\r\n        },)\r\n      }\r\n    },\r\n    mod(row){\r\n      console.log(row)\r\n\r\n      this.form.id=row.id\r\n      this.form.name=row.name\r\n      this.form.contact=row.contact\r\n      this.form.unitNumber=row.unitNumber\r\n      this.form.roomNumber=row.roomNumber\r\n      this.form.moveInDate=row.moveInDate\r\n      this.form.moveOutDate=row.moveOutDate\r\n\r\n      this.centerDialogVisible = true;\r\n    },\r\n    doSave(){\r\n      this.$axios.post(this.$httpUrl + '/resident/save',this.form).then(res=>{\r\n        console.log(res);\r\n        if(res.data.code==200){\r\n          this.$message({\r\n            message: '操作成功！',\r\n            type: 'success'\r\n          });\r\n          this.centerDialogVisible = false\r\n          this.loadPost()\r\n          this.resetFrom()\r\n        }else if(res.data.code==400){\r\n          if(res.data.data==\"User is not logged in\"){\r\n            this.$message({\r\n              message: '操作失败！',\r\n              type: 'error'\r\n            });\r\n          } else if(res.data.data==\"Data already exists\"){\r\n            this.$message({\r\n   